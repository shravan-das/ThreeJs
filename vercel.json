{
  "version": 2,
  "builds": [
    {
      "src": "client/dist/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*\\.(js|css|png|jpe?g|gif|svg|woff2?|ttf|eot))",
      "dest": "/client/dist/$1"
    },
    {
      "src": "/(.*)(/)?",
      "dest": "/client/dist/index.html"
    }
  ]
}
